@startuml

object Job{
  +Kind: String
}

object JobInstance {
  +Kind: String
  +UUID: uuid.UUID
  +status: String
  +createdAt: DateTime
  +updatedAt: DateTime
}

JobInstance --|> Job

object Manager {
  +Kind: String
  +UUID: uuid.UUID
  +status: String
  +createdAt: DateTime
  +updatedAt: DateTime
}

@enduml